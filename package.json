{
  "name": "g5-asset-scraper",
  "version": "1.1.0",
  "description": "Scrape assets and upload to Cloudinary",
  "main": "dist/index.js",
  "config": {
    "imageName": "asset-scraper",
    "imageRepo": "g5opex/asset-scraper"
  },
  "scripts": {
    "build": "babel -d ./dist ./src",
    "prebuild": "rm -rf dist/*",
    "docker:build": "docker build -t $npm_package_config_imageName .",
    "docker:tag": "docker tag $npm_package_config_imageName $npm_package_config_imageRepo:$npm_package_version",
    "docker:push": "docker push $npm_package_config_imageRepo:$npm_package_version",
    "start": "NODE_ENV=production node dist/index.js",
    "prestart": "npm run build",
    "dev": "nodemon --exec babel-node src/index.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylrhas/g5-asset-scraper.git"
  },
  "author": "Tyler Hasenoehrl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tylrhas/g5-asset-scraper/issues"
  },
  "homepage": "https://github.com/tylrhas/g5-asset-scraper#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.5",
    "cloudinary": "^1.23.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "jest": "^26.5.3",
    "nodemon": "^2.0.4"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "__tests__/config/"
    ]
  }
}
